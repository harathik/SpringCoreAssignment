<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd 
    http://www.springframework.org/schema/context/ 
    http://www.springframework.org/schema/context/spring-context.xsd">
    
    <beans>
    <bean id="Customer" class="ClassCustomer">
    
     <property name="custId" value="509"></property>
     <property name="custName" value="harathi"></property>
        <property name="custCont" value="8978867628"></property>
        <property name="custAddr" ref="Address"></property>
    
    </bean>
    
    
    
    <bean id="Address" class="ClassAddress">
      <property name="street" value="Benzcircle"></property>
    <property name="city" value="Vijayawada"></property>
        <property name="zip" value="520007"></property>
        <property name="country" value="India"></property>
    
    </bean>
    
    
    
    </beans>
    
    </beans>